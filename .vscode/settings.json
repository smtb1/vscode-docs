// Place your settings in this file to overwrite default and user settings.
{
    "editor.wordWrap": "on",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "**/toc.json": "jsonc"
    },
    "markdown.validate.enabled": true,
    "markdown.validate.ignoredLinks": [
        "/Download"
    ],
    "cSpell.enableFiletypes": [
        "!swift",
        "jsx-tags",
        "julia",
        "juliamarkdown",
        "lua",
        "powershell",
        "properties",
        "shaderlab",
        "sql",
        "tex",
        "vb",
        "xml",
        "xsl"
    ]
}